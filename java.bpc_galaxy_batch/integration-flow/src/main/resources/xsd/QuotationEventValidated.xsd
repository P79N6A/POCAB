                  <xs:documentation>Identifiant du fichier</xs:documentation> 
                </xs:annotation> 
              </xs:element>  
              <xs:element name="CreatnDate" type="ISODateTime" minOccurs="0"> 
                <xs:annotation> 
                  <xs:documentation>Date de création</xs:documentation> 
                </xs:annotation> 
              </xs:element> 
            </xs:sequence> 
          </xs:complexType> 
        </xs:element> 
      </xs:sequence> 
    </xs:complexType> 
