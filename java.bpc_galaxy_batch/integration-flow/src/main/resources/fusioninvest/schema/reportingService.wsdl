			</wsdl:input>
			<wsdl:output>
				<soapwsdl:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="ReportingService">
		<wsdl:port binding="tns:ReportingServiceSOAP" name="ReportingServicePort">
			<soapwsdl:address location="http://localhost:9080/ReportingWsHttpRouter/ReportingServicePort"/>
		</wsdl:port>
	</wsdl:service>

