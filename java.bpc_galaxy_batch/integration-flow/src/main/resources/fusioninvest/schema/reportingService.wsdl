		</xsd:schema>
		
		<xsd:schema attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://reportingService.soa.sophis.net" xmlns:authUtil="http://www.sophis.net/authentication" xmlns:reportService="http://www.sophis.net/reportingService">

			<xsd:import namespace="http://www.sophis.net/reportingService" schemaLocation="ReportingService.xsd"/>
			<xsd:import namespace="http://www.sophis.net/authentication" schemaLocation="authUtil.xsd"/>
			
			<xsd:simpleType name="ErrorCode">
				<xsd:annotation>
					<xsd:documentation source="http://www.sophis.net" xml:lang="en">
							0 	= success
							1 	= not logged in
							2 	= already logged in
							3 	= no license available
							4 	= reporting service error
							5	= other error; a reason is provided</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="SUCCESS"/>
					<xsd:enumeration value="NOTLOGGEDIN"/>
					<xsd:enumeration value="ALREADYLOGGED"/>
					<xsd:enumeration value="NOLICENSE"/>
					<xsd:enumeration value="REPORTINGERROR"/>
					<xsd:enumeration value="MISC"/>
				</xsd:restriction>
			</xsd:simpleType>

			<xsd:complexType name="Error">
				<xsd:sequence>
					<xsd:element name="code" type="tns:ErrorCode"/>
					<xsd:element minOccurs="0" name="reason" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation source="http://www.sophis.net" xml:lang="en">defined only if errorCode=3</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="GetReport">
				<xsd:sequence>
					<xsd:element ref="authUtil:token"/>
					<xsd:element ref="reportService:reportRequest"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="getReportInput" type="tns:GetReport"/>
			
			<xsd:complexType name="GetReportResponse">
				<xsd:sequence>
					<xsd:element minOccurs="0" ref="reportService:message"/>
					<xsd:element name="error" type="tns:Error"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="getReportOutput" type="tns:GetReportResponse"/>

			<xsd:complexType name="GetReportsList">
				<xsd:sequence>
					<xsd:element ref="authUtil:token"/>
					<xsd:element ref="reportService:reportsListRequest"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="getReportsListInput" type="tns:GetReportsList"/>

			<xsd:complexType name="GetReportsListResponse">
				<xsd:sequence>
					<xsd:element minOccurs="0" ref="reportService:message"/>
					<xsd:element name="error" type="tns:Error"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="getReportsListOutput" type="tns:GetReportsListResponse"/>

			<xsd:complexType name="ProcessRaw">
				<xsd:sequence>
					<xsd:element ref="authUtil:token"/>
					<xsd:element name="message" type="xsd:string"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="processRawInput" type="tns:ProcessRaw"/>

			<xsd:complexType name="ProcessRawResponse">
				<xsd:sequence>
					<xsd:element minOccurs="0" name="response" type="xsd:string"/>
					<xsd:element name="error" type="tns:Error"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="processRawOutput" type="tns:ProcessRawResponse"/>

			<xsd:complexType name="Login">
				<xsd:sequence>
					<xsd:element name="user" type="xsd:string"/>
					<xsd:element name="password" type="xsd:string"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="authUtil:property"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="loginInput" type="tns:Login"/>
			
			<xsd:complexType name="LoginResponse">
				<xsd:sequence>
					<xsd:element minOccurs="0" ref="authUtil:token"/>
					<xsd:element name="error" type="tns:Error"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="loginOutput" type="tns:LoginResponse"/>

			<xsd:complexType name="Logout">
				<xsd:sequence>
					<xsd:element ref="authUtil:token"/>
					<xsd:element name="user" type="xsd:string"/>
					<xsd:element name="password" type="xsd:string"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="authUtil:property"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="logoutInput" type="tns:Logout"/>

			<xsd:complexType name="LogoutResponse">
				<xsd:sequence>
					<xsd:element name="error" type="tns:Error"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:element name="logoutOutput" type="tns:LogoutResponse"/>
		</xsd:schema>	
	</wsdl:types>

	<wsdl:message name="getReportRequest">
		<wsdl:part element="tns:getReportInput" name="parameters"/>
	</wsdl:message>
	<wsdl:message name="getReportResponse">
		<wsdl:part element="tns:getReportOutput" name="parametersResponse"/>
	</wsdl:message>
	<wsdl:message name="getReportsListRequest">
