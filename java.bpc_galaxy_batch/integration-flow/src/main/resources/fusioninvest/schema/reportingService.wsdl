	<wsdl:message name="logoutResponse">
		<wsdl:part element="tns:logoutOutput" name="parametersResponse"/>
	</wsdl:message>

	<wsdl:portType name="ReportingService">
		<wsdl:operation name="getReport">
			<wsdl:input message="tns:getReportRequest"/>
			<wsdl:output message="tns:getReportResponse"/>
		</wsdl:operation>
		<wsdl:operation name="getReportsList">
