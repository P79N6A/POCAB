      <xsd:element minOccurs="0" maxOccurs="1" name="useFeesFrom" type="xsd:int"/>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="brokerFees" type="tns:BrokerFee"/>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="marketFees" type="tns:MarketFee"/>
    </xsd:sequence>
    <xsd:attribute default="true" name="overwrite" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation source="http://www.sophis.net" xml:lang="en">
				if true (default value if not specified), overwrite the contact list with new list
				if false, complete the contact list with new list
				</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
