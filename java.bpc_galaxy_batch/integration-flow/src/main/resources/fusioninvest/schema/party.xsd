  <xsd:sequence>
    <xsd:element name="lei" type="xsd:string" minOccurs="0" />
    <xsd:element name="usiNamespace" type="xsd:string" minOccurs="0" />
    <xsd:element name="jurisdiction" type="xsd:string" minOccurs="0"/>
