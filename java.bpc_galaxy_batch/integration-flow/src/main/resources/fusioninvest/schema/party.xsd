  <xsd:complexType name="AdvancedData" mixed="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">unbound list of non mandatory information of the party including settlement instructiosn, document generation, MiFID, agreement, netting rule and toolkitable tab</xsd:documentation>
