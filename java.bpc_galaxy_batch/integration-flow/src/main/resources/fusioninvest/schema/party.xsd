  <xsd:complexType name="BrokerFee">
    <xsd:annotation>
      <xsd:documentation source="http://www.sophis.net" xml:lang="en">
			group messages that define a the rule for broker fee
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="numero" type="xsd:int"/>
      <xsd:element name="option" type="xsd:int"/>
      <xsd:element name="detailedFeeName" type="xsd:string" default="None"/>
      <xsd:element name="currency" type="xsd:int"/>
      <xsd:element name="market" type="xsd:int"/>
      <xsd:element name="entity" type="xsd:int"/>
      <xsd:element name="allotment" type="xsd:int"/>
      <xsd:element name="kindofFees" type="xsd:string"/>
      <xsd:element name="amount" type="xsd:string"/>
      <xsd:element name="marginCall" type="xsd:double"/>
      <xsd:element name="minimum" type="xsd:double"/>
      <xsd:element name="depositary" type="xsd:int" default="-1"/>
      <xsd:element name="broker" type="xsd:int" default="-1"/>
      <xsd:element name="businessEvent" type="xsd:int" default="-1"/>
      <xsd:element name="underlyingAllotment" type="xsd:int" default="-1"/>
      <xsd:element name="underlyingCurrency" type="xsd:int" default="-1"/>
      <xsd:element name="underlyingMarket" type="xsd:int" default="-1"/>
      <xsd:element name="underlyingReference" type="xsd:int" default="-1" />
