          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="39"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="name">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation xml:lang="en">name of the contact person(s) at the party</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="39"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="address">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation xml:lang="en">address of the contact person(s) at the party</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="254"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="phone">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation xml:lang="en">phone number of the contact person(s) at the party</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="23"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="fax">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation xml:lang="en">fax number of the contact person(s) at the party</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="23"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="telex">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation xml:lang="en">telex details of the contact person(s) at the party</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="23"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="email">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation xml:lang="en">email address of the contact person(s) at the party</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="119"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" default="false" name="defaultClient" type="xsd:boolean"/>
      <xsd:element minOccurs="0" name="allotment" type="xsd:normalizedString"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Contacts">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">Contact information of the party </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="contact" type="tns:Contact"/>
    </xsd:sequence>
    <xsd:attribute default="true" name="overwrite" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation source="http://www.sophis.net" xml:lang="en">
				if true (default value if not specified), overwrite the contact list with new list
				if false, complete the contact list with new list
				</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="CustomProperty">
    <xsd:annotation>
      <xsd:documentation source="http://www.sophis.net" xml:lang="en">
		custom defined properties of party
		</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="tns:StringBounded140">
        <xsd:attribute name="name" type="tns:StringBounded20"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Custom">
    <xsd:annotation>
      <xsd:documentation source="http://www.sophis.net" xml:lang="en">
				custom attributes for the party
				</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="property" type="tns:CustomProperty"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="StringBounded23">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">bounded string element of custom attributes. 23 characters</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:normalizedString">
      <xsd:maxLength value="23"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StringBounded19">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">bounded string element of custom attributes. 19 characters</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="19"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StringBounded20">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">bounded string element of custom attributes. 20 characters</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="20"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="StringBounded140">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">bounded string element of custom attributes. 140 characters</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="140"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="SettlementInstruction">
    <xsd:annotation>
      <xsd:documentation source="http://www.fpml.org" xml:lang="en">
			 An explicit specification of how a currency payment is to be made, when the payment is not netted and the route is other than the recipient's standard settlement instruction.
				</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ident" type="xsd:int"/>
	  <xsd:element name="ssiPathId" type="xsd:int" minOccurs="0"/>
      <xsd:element name="financialInstitution" type="xsd:int" minOccurs="0"/>
      <xsd:element name="partyRole" type="xsd:int" minOccurs="0"/>
      <xsd:element name="workflow" type="xsd:int" minOccurs="0"/>

	  <xsd:element name="workflowName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="deliveryType" type="xsd:int" minOccurs="0"/>
      <xsd:element name="validFrom" type="xsd:date" minOccurs="0"/>
      <xsd:element name="validTo" type="xsd:date" minOccurs="0"/>
      <xsd:element name="currency" type="xsd:int" minOccurs="0"/>
      <xsd:element name="market" type="xsd:int" minOccurs="0"/>
      <xsd:element name="depositary" type="xsd:int" minOccurs="0"/>
      <xsd:element name="sign" type="xsd:int" minOccurs="0"/>
      <xsd:element name="allotment" type="xsd:int" minOccurs="0"/>
      <xsd:element name="accountType" type="xsd:int" minOccurs="0"/>
      <xsd:element name="paymentMethod" type="xsd:int" minOccurs="0"/>
      <xsd:element name="treasuryID" type="xsd:int" minOccurs="0"/>
      <xsd:element name="underlyingAllotment" type="xsd:int" minOccurs="0"/>
      <xsd:element name="underlyingCurrency" type="xsd:int" minOccurs="0"/>
      <xsd:element name="underlyingMarket" type="xsd:int" minOccurs="0"/>
      <xsd:element name="businessEvent" type="xsd:int" minOccurs="0"/>
      <xsd:element name="settlementPlace" type="xsd:int" minOccurs="0"/>
      <xsd:element name="accountName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="custodianID" type="xsd:int" minOccurs="0"/>
      <xsd:element name="custodianCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="accountCustodian" type="xsd:string" minOccurs="0"/>
      <xsd:element name="agentID" type="xsd:int" minOccurs="0"/>
      <xsd:element name="agentCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="accountAgent" type="xsd:string" minOccurs="0"/>
      <xsd:element name="countryCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="condition" type="xsd:string" minOccurs="0"/>
      <xsd:element name="condition2" type="xsd:string" minOccurs="0"/>
      <xsd:element name="condition3" type="xsd:string" minOccurs="0"/>
      <xsd:element name="userColumnsData" type="tns:UserColumnsData" minOccurs="0"/>
	  <xsd:element name="carveOut" type="xsd:int" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DocumentGenerationRule">
    <xsd:annotation>
      <xsd:documentation source="http://www.sophis.net" xml:lang="en">
