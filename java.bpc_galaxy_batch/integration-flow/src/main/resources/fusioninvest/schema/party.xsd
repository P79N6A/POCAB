      <xsd:element minOccurs="0" maxOccurs="unbounded" name="settlementInstruction" type="tns:SettlementInstruction"/>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="documentGenerationRule" type="tns:DocumentGenerationRule"/>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="agreement" type="tns:Agreement"/>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="nettingRule" type="tns:NettingRule"/>
      <xsd:element minOccurs="0" name="mifid" type="tns:MiFID"/>
