	  <xsd:element name="clearingMember" type="xsd:int" minOccurs="0"/>
      <xsd:element name="userColumnsData" type="tns:UserColumnsData" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="NettingRule">
    <xsd:annotation>
      <xsd:documentation source="http://www.sophis.net" xml:lang="en">
