  <xsd:complexType name="Agreement">
    <xsd:annotation>
      <xsd:documentation source="http://www.sophis.net" xml:lang="en">specify the legal compliance characteristics of a master agreement which is agreed between the entity and the Party. This agreement is used for each confirmation message that is sent to the Client.</xsd:documentation>
      <xsd:documentation source="http://www.fpml.org" xml:lang="en">An agreement that references the related party.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="allotment" type="xsd:int" minOccurs="0"/>
      <xsd:element name="allotmentString" type="xsd:string" minOccurs="0" />
      <xsd:element name="childAllotment" type="xsd:string" minOccurs="0" />
	  <xsd:element name="terminationCurrency" type="xsd:string" minOccurs="0"/>
      <xsd:element name="entity" type="xsd:int" minOccurs="0"/>
      <xsd:element name="effectiveDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="amendmentDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="agreementVersionDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="agreement" type="xsd:string" minOccurs="0"/>
      <xsd:element name="law" type="xsd:string" minOccurs="0"/>
