    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="XXX"/>
      <xsd:enumeration value="Retail"/><xsd:enumeration value="Professional"/>

      <xsd:enumeration value="Eligible Counterparty"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Reporter">
    <xsd:annotation>
      <xsd:documentation source="http://www.sophis.net" xml:lang="en">
