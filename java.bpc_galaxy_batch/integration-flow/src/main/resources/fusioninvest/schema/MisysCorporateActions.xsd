			<xsd:documentation xml:lang="en">Split Corporate Action Details</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="mca:CorporateActionDetails_desc">
				<xsd:all>
					<xsd:element name="AnnouncementDate" type="xsd:date" minOccurs="0" nillable="true"/>
					<xsd:element name="RecordDate" type="xsd:date" minOccurs="0" nillable="true"/>
					<xsd:element name="ExDate" type="xsd:date" minOccurs="0" nillable="true"/>
					<xsd:element name="EffectiveDate" type="xsd:date" />
					<xsd:element name="PaymentDate" type="xsd:date" minOccurs="0" nillable="true"/>
					<xsd:element name="ImpactListedInstruments" type="mca:ImpactListed_desc" minOccurs="0" />
				</xsd:all>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>

