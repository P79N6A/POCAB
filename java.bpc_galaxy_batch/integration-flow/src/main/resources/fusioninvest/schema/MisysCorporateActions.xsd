	<xsd:complexType name="CorporateAction_base">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Base type extended by all custom Corporate Actions</xsd:documentation>
		</xsd:annotation>
	</xsd:complexType>
	
	<xsd:complexType name="CA_Split_desc">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">"CA to XML" description of a corporate action - SPLIT</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="mca:CorporateAction_base">
				<xsd:all>
					<xsd:element name="GeneralInformation" type="mca:GeneralInformation_desc" />
					<xsd:element name="UnderlyingSecurities" type="mca:UnderlyingSecurities_desc" />
					<xsd:element name="CorporateActionDetails" type="mca:SplitCorporateActionDetails_desc" />
					<xsd:element name="CorporateActionOptions" type="mca:Split_Options_desc" />
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="CA_ReverseSplit_desc">
