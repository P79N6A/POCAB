				<x:EnumDoc value="3" desc="T2" />
				<x:EnumDoc value="4" desc="T3" />
				<x:EnumDoc value="5" desc="T4" />
				<x:EnumDoc value="6" desc="Future" />
				<x:EnumDoc value="7" desc="WhenIssued" />
				<x:EnumDoc value="8" desc="SellersOption" />
				<x:EnumDoc value="9" desc="T5" />
