  <xs:complexType name="QuotationType"> 
    <xs:annotation> 
      <xs:documentation>Objet : Cours</xs:documentation> 
    </xs:annotation>  
    <xs:sequence> 
      <xs:element name="QuotatnIdntfctn"> 
        <xs:complexType> 
          <xs:sequence> 
            <xs:choice> 
