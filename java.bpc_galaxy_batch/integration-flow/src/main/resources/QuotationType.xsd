                </xs:element>  
                <xs:element name="PlaceOfListng" type="MICIdentifier" minOccurs="0"> 
                  <xs:annotation> 
                    <xs:documentation>Place de cotation</xs:documentation> 
                  </xs:annotation> 
                </xs:element> 
              </xs:sequence>  
              <xs:element name="FutIdntfctn"> 
                <xs:annotation> 
                  <xs:documentation>Identification d'un Future</xs:documentation> 
                </xs:annotation>  
                <xs:complexType> 
                  <xs:sequence> 
                    <xs:element name="MarktIdntfctn" type="MICIdentifier"> 
                      <xs:annotation> 
                        <xs:documentation>A standardized code used to identify places of trade, ie, stock exchanges, regulated (eg, Electronic Trading Platforms (ECN)) and unregulated (eg, Automated Trading Systems (ATS)) markets, as sources of prices and related information in order to facilitate automated processing.</xs:documentation> 
                      </xs:annotation> 
                    </xs:element>  
                    <xs:element name="CntrctName" type="Name"> 
                      <xs:annotation> 
                        <xs:documentation>Nom d'un contrat future ou option</xs:documentation> 
                      </xs:annotation> 
                    </xs:element>  
                    <xs:element name="ExpiryDate" type="ISODateTime"> 
                      <xs:annotation> 
                        <xs:documentation>Date à laquelle expire un privilège (option, droit...)</xs:documentation> 
                      </xs:annotation> 
                    </xs:element> 
                  </xs:sequence> 
                </xs:complexType> 
              </xs:element>  
              <xs:element name="OptIdntfctn"> 
                <xs:annotation> 
                  <xs:documentation>Identification d'une Option</xs:documentation> 
                </xs:annotation>  
                <xs:complexType> 
                  <xs:sequence> 
                    <xs:element name="MarktIdntfctn" type="MICIdentifier"> 
                      <xs:annotation> 
                        <xs:documentation>A standardized code used to identify places of trade, ie, stock exchanges, regulated (eg, Electronic Trading Platforms (ECN)) and unregulated (eg, Automated Trading Systems (ATS)) markets, as sources of prices and related information in order to facilitate automated processing.</xs:documentation> 
                      </xs:annotation> 
                    </xs:element>  
                    <xs:element name="CntrctName" type="Name"> 
                      <xs:annotation> 
                        <xs:documentation>Nom d'un contrat future ou option</xs:documentation> 
                      </xs:annotation> 
                    </xs:element>  
                    <xs:element name="CntrctVrsNumb"> 
                      <xs:annotation> 
                        <xs:documentation>Numéro de version de l'option ou de la tranche de l'instrument financier</xs:documentation> 
                      </xs:annotation>  
                      <xs:simpleType> 
                        <xs:restriction base="xs:string"> 
                          <xs:maxLength value="3"/> 
                        </xs:restriction> 
                      </xs:simpleType> 
                    </xs:element>  
                    <xs:element name="ExpiryDate" type="ISODateTime"> 
                      <xs:annotation> 
                        <xs:documentation>Date à laquelle expire un privilège (option, droit...)</xs:documentation> 
                      </xs:annotation> 
                    </xs:element>  
                    <xs:element name="OptType" type="OptionTypeCode"> 
                      <xs:annotation> 
                        <xs:documentation>Spécifie si une option est une option d'achat (Call, droit d'acheter le sous-jacent) ou de vente (Put, droit de vendre le sous-jacent)</xs:documentation> 
                      </xs:annotation> 
                    </xs:element>  
                    <xs:element name="ExrcisePrice" type="Price"> 
                      <xs:annotation> 
