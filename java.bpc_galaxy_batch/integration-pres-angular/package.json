{
  "name": "Archetype",
  "version": "0.0.1",
  "license": "BNP",
  "scripts": {
    "lint": "tslint --project tsconfig.json -e 'node_modules/**'",
    "lint:fix": "npm run lint -- --fix",
    "cleanup": "rimraf target/{aot,www}",
    "clean-www": "rimraf target//www/app/{src,target/}",
    "start": "npm run webpack:dev",
    "serve": "npm run start",
    "test": "jest --coverage --logHeapUsage -w=2 --config src/test/jest.conf.js",
    "test:watch": "npm test -- --watch --clearCache",
    "build": "npm run webpack:prod",
    "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --inline --hot --port=4200 --watch-content-base --profile --progress --env.stats=normal",
    "webpack:dev-verbose": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --inline --hot --port=4200 --watch-content-base --profile --progress --env.stats=normal",
    "webpack:build:main": "npm run webpack -- --config webpack/webpack.dev.js --env.stats=minimal",
    "webpack:build": "npm run cleanup && npm run webpack:build:main",
    "webpack:prod:main": "webpack --config webpack/webpack.prod.js",
    "webpack:prod": "npm run cleanup && npm run webpack:prod:main && npm run clean-www",
    "webpack:test": "npm run test",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/cli": "6.2.4",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/router": "6.1.10",
    "@types/file-saver": "1.3.1",
    "core-js": "2.5.7",
    "file-saver": "1.3.8",
    "intl": "1.2.5",
    "primeng": "6.1.5",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.3",
    "@angular/compiler-cli": "6.1.10",
    "@ngtools/webpack": "7.0.2",
    "@types/chai": "4.1.7",
    "@types/chai-string": "1.4.1",
    "@types/jest": "23.3.8",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.0",
    "angular-router-loader": "0.8.5",
    "angular2-template-loader": "0.6.2",
    "browser-sync": "2.26.3",
    "browser-sync-webpack-plugin": "2.2.2",
    "cache-loader": "1.2.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.5.0",
    "codelyzer": "4.5.0",
    "css-loader": "1.0.0",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "0.4.10",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "jest-junit": "5.2.0",
    "jest-preset-angular": "6.0.1",
    "jest-sonar-reporter": "2.0.0",
    "mini-css-extract-plugin": "0.4.4",
    "mocha": "5.2.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "rimraf": "2.6.2",
    "simple-progress-webpack-plugin": "1.1.2",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.1.0",
    "thread-loader": "1.2.0",
    "to-string-loader": "1.1.5",
    "ts-loader": "5.2.2",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-loader": "3.5.4",
    "typescript": "2.9.2",
    "webpack": "4.21.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9",
    "webpack-merge": "4.1.4",
    "webpack-notifier": "1.7.0",
    "webpack-visualizer-plugin": "0.1.11",
    "workbox-webpack-plugin": "3.6.2",
